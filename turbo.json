{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "format": {
      "cache": false
    },
    "test:unit": {
      "cache": false
    },
    "test:cov": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build",
        "format",
        "lint",
        "test:unit"
      ],
      "outputs": [
        "dist/**"
      ]
    }
  }
}
